{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/webextensions/tree/main/auto-close-tabs",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "description": "automatically close tabs which have not been accessed for a time",
  "icons": {
    "256": "icon.png"
  },
  "manifest_version": 2,
  "name": "Auto Close Tabs",
  "permissions": [
    "contextualIdentities",
    "cookies",
    "bookmarks",
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "incognito": "not_allowed",
  "version": "1.8.35"
}
