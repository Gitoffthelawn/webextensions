{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/webextensions/tree/main/sources/load-background-tabs-on-select",
  "description": "Prevents background tabs from loading until they have been activated",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Load Background Tabs on Select",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage", "tabs"],
  "version": "1.2.13"
}
