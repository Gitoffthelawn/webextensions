{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/webextensions/tree/main/toolbar-countdown",
  "description": "Shows a the most significant part (days,hours,minutes or seconds) of a remaining custom countdown on the toolbar",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "icon.png"
  },
  "manifest_version": 2,
  "name": "Toolbar Countdown",
  "permissions": ["storage"],
  "optional_permissions": ["notifications"],
  "version": "1.1.11"
}
